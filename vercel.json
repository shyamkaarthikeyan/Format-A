{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "functions": {
    "server/index.ts": {
      "runtime": "@vercel/node@3.0.7"
    }
  },
  "env": {
    "PYTHON_PATH": "/usr/bin/python3",
    "SKIP_DOCX2PDF": "true"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/index.ts"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ]
}